---
interface Props {
    title: string;
    href: string;
}

const { href, title } = Astro.props;
---

<li class="list-none">
    <a
        href={href}
        class="inline-block px-2 py-1 text-gray-900 font-medium border-0 no-underline hover:text-custom-purple focus:text-custom-purple active:text-custom-purple focus:outline-none"
        style="text-decoration:none!important;border:0!important;background:transparent!important;"
    >{title}</a>
</li>
